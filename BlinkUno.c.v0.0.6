#include <stdint.h>
#include <stdlib.h>
#include <stdio.h>
#include <util/delay.h>
#include <avr/io.h>

//anolog in
//C0-5=0-5
//digital in/out
//Di=i [0-7]
//B0=8, B1=9, B2=10, B3=11, B4=12, B5=13 [! B6&7 crystal !!]
#define LED_DDR  DDRB
#define LED_PORT PORTB
#define LED1     PORTB1
#define LED2     PORTB2

int main(void)
{
  LED_DDR|=_BV(LED1)|_BV(LED2);//LED output
  while(1)
  {
    LED_PORT|=_BV(LED1);//LED on
    _delay_ms(50);
    LED_PORT&=~_BV(LED1);//LED off
    LED_PORT|=_BV(LED2);//LED on
    _delay_ms(50);
    LED_PORT|=_BV(LED2);//LED on
    _delay_ms(50);
    LED_PORT&=~_BV(LED1);//LED off
    LED_PORT&=~_BV(LED2);//LED off
    _delay_ms(1000);
  }
  return (0);
}


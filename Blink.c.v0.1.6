#include <stdint.h>
#include <stdlib.h>
#include <stdio.h>
#include <util/delay.h>
#include <avr/io.h>

#define LED_DDR  DDRB
#define LED_PORT PORTB
//ArduinoMega pin 13
#define LED      PB7

int main(void)
{
  LED_DDR=255;//LED output
  while(1)
  {
    LED_PORT=255;//LED on
    _delay_ms(50);
    LED_PORT=0;//LED off
    _delay_ms(1000);
  }
  return (0);
}


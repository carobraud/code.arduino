#include <stdint.h>
#include <stdlib.h>
#include <stdio.h>
#include <util/delay.h>
#include <avr/io.h>

#define LED_DDR  DDRB
#define LED_PORT PORTB
//ArduinoMega pin 13
#define LED      PB7

int main(void)
{
  LED_DDR=255;//LED output
  while(1)
  {
    LED_PORT|=_BV(LED);//LED on
    _delay_ms(5);
    LED_PORT&=~_BV(LED);//LED off
    _delay_ms(100);
  }
  return (0);
}

